{
  "name": "agno-client-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "Independent open-source client library for Agno agents",
  "author": "antipopp",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter './packages/*' build",
    "dev": "pnpm -r --parallel --filter './packages/*' dev",
    "clean": "pnpm -r exec rm -rf dist node_modules",
    "typecheck": "pnpm -r --filter './packages/*' typecheck"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "shadcn": "^3.5.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.0.0"
}
