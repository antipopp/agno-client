{
  "name": "@antipopp/agno-client",
  "version": "0.6.1",
  "description": "Core client library for Agno agents with streaming support and HITL frontend tool execution",
  "author": "antipopp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antipopp/agno-client.git",
    "directory": "packages/core"
  },
  "keywords": [
    "agno",
    "agent",
    "ai",
    "client",
    "streaming",
    "hitl",
    "tool-execution"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antipopp/agno-types": "workspace:*",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
