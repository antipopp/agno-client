{
  "name": "@antipopp/agno-react",
  "version": "0.1.0",
  "description": "React hooks for Agno client with frontend tool execution (HITL) support",
  "author": "antipopp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antipopp/agno-client.git",
    "directory": "packages/react"
  },
  "keywords": [
    "agno",
    "agent",
    "ai",
    "react",
    "hooks",
    "hitl",
    "tool-execution",
    "streaming"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --external react --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antipopp/agno-client": "workspace:*",
    "@antipopp/agno-types": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
